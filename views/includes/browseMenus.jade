.browseWrapper
  .browse.browse-courses
    each course in modelCache.coursesArray
      div
        h2: a(href=course.url)= course.name
        if (course.notetypes)
          ul
            each notetype in course.notetypes
              li: a(href=course.notetypeUrl(notetype))= notetype.name
    div
      a.btn(href='/submit/note/') Add your notes

.browseWrapper
  .browse.browse-colleges
    div
      h2: a(href='/colleges/') Admissions Essays
      ul
        each college in modelCache.collegesArray.slice(0, 7)
          li: a(href=college.url, title='#{ college.name } Sample Essays') #{ college.shortName } Essays
    div
      ul
        each college in modelCache.collegesArray.slice(7, 15)
          li: a(href=college.url, title='#{ college.name } Sample Essays') #{ college.shortName } Essays
    div
      ul
        each college in modelCache.collegesArray.slice(15, 23)
          li: a(href=college.url, title='#{ college.name } Sample Essays') #{ college.shortName } Essays
    div
      ul
        each college in modelCache.collegesArray.slice(23)
          li: a(href=college.url, title='#{ college.name } Sample Essays') #{ college.shortName } Essays

      a.btn(href='/submit/essay/') Add your essay