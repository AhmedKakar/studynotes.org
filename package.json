{
  "name": "studynotes",
  "version": "0.2.1",
  "description": "The StudyNotes.org website. Helping students learn faster and better.",
  "homepage": "http://www.apstudynotes.org",
  "main": "index.js",
  "dependencies": {
    "async": "~0.2.9",
    "bcrypt": "~0.7.7",
    "browserify-shim": "~3.1.4",
    "browserify": "~3.19.1",
    "bytes": "~0.2.1",
    "closurecompiler": "~1.3.0",
    "connect-flash": "~0.1.1",
    "connect-mongo": "~0.4.0",
    "connect-slashes": "~1.2.0",
    "debug": "~0.7.2",
    "express": "~3.4.0",
    "font-awesome": "git://github.com/FortAwesome/Font-Awesome",
    "html-parser": "~0.7.0",
    "html-truncate": "~1.0.3",
    "http-status-codes": "~1.0.2",
    "humane-js": "~3.1.0",
    "jade": "~1.1.2",
    "jquery": "~2.1.0-beta3",
    "jquery-placeholder": "~2.0.7",
    "jsdom": "~0.8.10",
    "keymaster": "git://github.com/madrobby/keymaster",
    "lodash.throttle": "~2.4.1",
    "MD5": "~1.2.0",
    "mkdirp": "~0.3.5",
    "moment": "~2.5.0",
    "mongoose": "~3.8.1",
    "mongoose-validator": "~0.2.2",
    "netdna": "0.0.6",
    "nib": "~1.0.1",
    "node-uuid": "~1.4.1",
    "nodemailer": "~0.6.0",
    "once": "~1.3.0",
    "optimist": "~0.6.0",
    "passport": "~0.1.17",
    "passport-local": "~0.1.6",
    "posix": "~1.0.2",
    "purge-netdna": "~1.0.1",
    "stylus": "~0.42.0",
    "touch": "0.0.2",
    "transparency": "~0.9.14",
    "underscore": "~1.5.2",
    "underscore.string": "~2.3.3",
    "ws": "~0.4.31"
  },
  "devDependencies": {
    "formage": "2.x",
    "tape": "2.x",
    "nodemon": "~0.7.10"
  },
  "browser": {
    "ckeditor": "./lib/ckeditor/ckeditor.js",
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "jquery-placeholder": "./node_modules/jquery-placeholder/jquery.placeholder.js",
    "keymaster": "./node_modules/keymaster/keymaster.js",
    "select2": "./lib/select2/select2.js",
    "transparency": "./node_modules/transparency/dist/transparency.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "ckeditor": "CKEDITOR",
    "jquery": "jQuery",
    "jquery-placeholder": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "keymaster": "key",
    "select2": {
      "exports": "Select2",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "transparency": {
      "exports": "Transparency",
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "scripts": {
    "admin": "nodemon admin/index.js",
    "build-js": "browserify -r ./util-browser -r ./config-browser static/js/main.js > out/main.js && node ./bin/minify out/main.js",
    "build-css": "stylus -u nib stylus/main.styl -o out/ --include-css -c && node ./bin/transform-css.js out/main.css",
    "build": "npm run build-js && npm run build-css && node ./bin/postbuild.js",
    "watch-js": "watchify -r ./util-browser -r ./config-browser static/js/main.js -o out/main.js -dv",
    "watch-css": "stylus -u nib stylus/main.styl -o out/ --include-css -w",
    "watch": "npm run watch-js & npm run watch-css",
    "db-tunnel": "ssh -L 27017:localhost:27017 -N feross@athena",
    "db-local": "mongod",
    "start": "npm run db-tunnel & npm run watch & (sleep 2 && DEBUG=\"studynotes:*\" nodemon run.js)",
    "offline": "npm run db-local & npm run watch & (sleep 1 && DEBUG=\"studynotes:*\" nodemon run.js --offline)",
    "test": "tape test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feross/StudyNotes.git"
  },
  "keywords": [
    "AP notes",
    "college essays",
    "high school",
    "college prep"
  ],
  "author": "Feross Aboukhadijeh <feross@feross.org> (http://feross.org/)",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/feross/StudyNotes/issues"
  }
}
